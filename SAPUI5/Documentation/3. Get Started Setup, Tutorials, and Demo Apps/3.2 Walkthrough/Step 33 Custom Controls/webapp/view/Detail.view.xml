<mvc:View
  controllerName="zms.controller.Detail"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:wt="zms.control"
  xmlns:microchart="sap.suite.ui.microchart">
  <Page
    title="{i18n>detailPageTitle}"
    showNavButton="true"
    navButtonPress=".onNavBack">
    <ObjectHeader
      intro="{invoice>ShipperName}"
      title="{invoice>ProductName}"/>

    <HBox class="v">
      <microchart:InteractiveDonutChart displayedSegments="2" class="o">
        <microchart:segments>
          <microchart:InteractiveDonutChartSegment label="已设置" value="40" displayedValue="40%" color="Error"/>
          <microchart:InteractiveDonutChartSegment label="未设置" value="60" displayedValue="60%" color="Error"/>
        </microchart:segments>
      </microchart:InteractiveDonutChart>
    </HBox>


    <wt:ProductRating
      id="rating"
      class="sapUiSmallMarginBeginEnd"
      value="3"
      change=".onRatingChange"/>
  </Page>
</mvc:View>
