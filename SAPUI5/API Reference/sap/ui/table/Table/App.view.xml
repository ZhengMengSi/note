<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:t="sap.ui.table"
    controllerName="zms.App">
    <t:Table
        rows="{/rows}"
        selectionMode="None">
        <t:extension>
            <OverflowToolbar>
                <Title text="Title也是一个控件"/>
            </OverflowToolbar>
        </t:extension>
        <t:columns>
            <t:Column>
                <t:template>
                    <Text text="{name}"/>
                </t:template>
            </t:Column>
            <t:Column>
                <Label text="姓名"/>
                <t:template>
                    <Text text="{name}"/>
                </t:template>
            </t:Column>
            <t:Column>
                <Label text="姓名"/>
                <t:template>
                    <Text text="{age}"/>
                </t:template>
            </t:Column>
        </t:columns>
    </t:Table>
</mvc:View>